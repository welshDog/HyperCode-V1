{
  "name": "hypercode-research-db",
  "version": "1.0.0",
  "description": "HyperCode Research Database - Neurodivergent-first programming language with AI and quantum computing integration",
  "main": "hypercode_db.json",
  "scripts": {
    "validate": "node scripts/validate.js",
    "sync": "node scripts/sync-firebase.js",
    "dev": "npm run validate && npm run sync",
    "test": "npm run validate",
    "schema-check": "ajv validate -s hypercode_schema.json -d hypercode_db.json",
    "quality-check": "npm run validate",
    "backup": "cp hypercode_db.json hypercode_db.$(date +%Y%m%d_%H%M%S).backup.json",
    "restore": "npm run backup && git checkout hypercode_db.json",
    "version-bump": "node scripts/version-bump.js",
    "update-timestamp": "node scripts/update-timestamp.js",
    "merge-conflicts": "node scripts/resolve-conflicts.js"
  },
  "keywords": [
    "HyperCode",
    "neurodivergent",
    "programming-language",
    "ADHD",
    "autism",
    "dyslexia",
    "AI",
    "quantum-computing",
    "molecular-computing",
    "research",
    "PlankalkÃ¼l",
    "Brainfuck",
    "Befunge"
  ],
  "author": "HyperCode Community",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hypercode/research-db.git"
  },
  "bugs": {
    "url": "https://github.com/hypercode/research-db/issues"
  },
  "homepage": "https://github.com/hypercode/research-db",
  "devDependencies": {
    "ajv": "^8.12.0",
    "ajv-cli": "^5.0.0",
    "firebase": "^10.7.0",
    "firebase-admin": "^12.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "eslint": "^8.54.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "files": [
    "hypercode_db.json",
    "hypercode_schema.json",
    "scripts/",
    "README.md",
    ".github/workflows/"
  ]
}
